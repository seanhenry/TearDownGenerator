<idea-plugin>
  <id>codes.seanhenry.teardown</id>
  <name>Test Tear Down Generator for AppCode</name>
  <version>1.0</version>
  <vendor email="hello@seanhenry.codes" url="http://www.seanhenry.codes">Sean Henry</vendor>

  <description><![CDATA[
      Automatically generate tear down methods in your tests in one action.
      <ul>
        <li></li>
      </ul>
    ]]></description>

  <change-notes><![CDATA[
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="163.0"/>

  <actions>
  </actions>

  <depends>com.intellij.modules.appcode</depends>

  <project-components>
  </project-components>

  <extensions defaultExtensionNs="com.intellij">
    <localInspection implementationClass="codes.seanhenry.inspections.TearDownInspection"
                     displayName="Properties are not set to nil in tear down"
                     groupName="Swift"
                     enabledByDefault="true"/>
  </extensions>

</idea-plugin>